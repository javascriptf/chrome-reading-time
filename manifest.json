{
  "manifest_version": 3,
  "name": "Reading time",
  "version": "0.1",
  "description": "Adds the expected reading time to any blog or documentation page.",
  "content_scripts": [{
    "js": ["scripts/content.js"],
    "matches": [
      "*://*/*"
    ]
  }],
  "icons": {
    "16":  "icons/16.png",
    "32":  "icons/32.png",
    "64":  "icons/64.png",
    "128": "icons/128.png"
  }
}
